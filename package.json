{
  "name": "starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "postinstall": "husky install",
    "test": "jest --collectCoverage --passWithNoTests",
    "tsc": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint --cache 'src/**/*.ts?(x)'",
    "lint:fix": "yarn lint --fix",
    "generate-apollo-types": "apollo codegen:generate --target typescript --outputFlat src/apollo/generated",
    "generate-apollo-fragment-matcher": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@hookform/resolvers": "2.8.5",
    "@react-native-community/hooks": "2.8.1",
    "@react-navigation/bottom-tabs": "6.0.9",
    "@react-navigation/core": "6.1.0",
    "@react-navigation/material-top-tabs": "6.0.6",
    "@react-navigation/native": "6.0.6",
    "@react-navigation/native-stack": "6.2.5",
    "date-fns": "2.28.0",
    "i18next": "21.6.5",
    "intl-pluralrules": "1.3.1",
    "lodash": "4.17.21",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "5.0.1",
    "react": "17.0.2",
    "react-hook-form": "7.22.5",
    "react-i18next": "11.15.3",
    "react-native": "0.66.4",
    "react-native-bootsplash": "3.2.4",
    "react-native-dotenv": "3.3.1",
    "react-native-fast-image": "8.5.11",
    "react-native-flipper-apollo-devtools": "0.0.2",
    "react-native-gesture-handler": "2.1.0",
    "react-native-reanimated": "2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-switch-pro": "1.0.5",
    "react-native-webview": "11.15.0",
    "styled-components": "5.3.3",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@babel/runtime": "7.16.7",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@react-native-community/eslint-config": "3.0.1",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "@types/react-native": "0.66.11",
    "@types/styled-components-react-native": "5.1.3",
    "@types/yup": "0.29.13",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "@typescript-eslint/typescript-estree": "5.9.0",
    "apollo": "2.33.9",
    "babel-jest": "27.4.6",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.6.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "25.3.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.1.5",
    "metro-react-native-babel-preset": "0.66.2",
    "prettier": "2.5.1",
    "react-native-flipper": "0.127.0",
    "react-test-renderer": "17.0.2",
    "typescript": "4.5.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix"
    ],
    "*.{js,html,md,json}": [
      "prettier --write"
    ]
  }
}
